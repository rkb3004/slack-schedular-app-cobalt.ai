{
  "version": 2,
  "buildCommand": "chmod +x ./vercel-build.sh && ./vercel-build.sh",
  "outputDirectory": "dist",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://slack-schedular-app-cobalt-ai.onrender.com"
  }
}
